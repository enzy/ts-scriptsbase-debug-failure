{
	"compilerOptions": {
		"allowSyntheticDefaultImports": true,
		"declaration": true,
		"declarationMap": true,
		"downlevelIteration": true,
		"esModuleInterop": true,
		"experimentalDecorators": true,
		"importHelpers": true,
		"jsx": "react",
		"lib": ["dom", "es6", "scripthost", "dom.iterable"],
		"module": "esnext",
		"moduleResolution": "node",
		"sourceMap": true,
		"strict": true,
		"target": "es5"
	},
	"exclude": ["vendor", "node_modules"]
}
